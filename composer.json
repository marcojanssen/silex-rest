{
    "name": "marcojanssen/silex-rest",
    "description": "Silex REST skeleton",
    "keywords": ["silex", "rest", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Janssen",
            "email": "dev@marcojanssen.net"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "filp/whoops": "~1.1",
        "igorw/config-service-provider": "1.2.*",
        "marcojanssen/silex-rest-service-providers": "1.0.*",
        "marcojanssen/silex-routing-service-provider": "1.2.*",
        "marcojanssen/silex-service-register-provider": "1.1.*",
        "silex/silex": "~1.2",
        "symfony/monolog-bridge": "~2.3"
    },
    "scripts": {
        "post-install-cmd": [
            "chmod 777 app/cache",
            "chmod 777 app/logs"
        ],
        "post-update-cmd": [
            "chmod 777 app/cache",
            "chmod 777 app/logs"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "psr-0": {
            "Example\\": "src/"
        }
    }
}
